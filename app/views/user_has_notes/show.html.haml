- model_class = UserHasNote
.page-header
  %h1=t '.title', :default => model_class.model_name.human

%p
  %strong= model_class.human_attribute_name(:user_id) + ':'
  %br
  = @user_has_note.user_id
%p
  %strong= model_class.human_attribute_name(:note_id) + ':'
  %br
  = @user_has_note.note_id

.form-actions
  = link_to t('.back', :default => t("helpers.links.back")), user_has_notes_path, :class => 'btn'
  = link_to t('.edit', :default => t("helpers.links.edit")), edit_user_has_note_path(@user_has_note), :class => 'btn btn-primary'
  = link_to t('.destroy', :default => t("helpers.links.destroy")), user_has_note_path(@user_has_note), :method => "delete", :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, :class => 'btn btn-danger'
